{{#liquid-bind model use='toUp'}}
  <div class="scrollable-menus">
    <h4>{{model.firstObject.menu2}}</h4>

    <div>
      {{#each model as |dataSet|}}
        {{!-- <a class="item" href={{concat dataSet.dataBrowserEndpoint 'select * from ' dataSet.table_name '&format=csv' }} target="_blank"> --}}
        {{#link-to 'datasets' dataSet.id}}
          <div class="content">
            <div class="header">{{dataSet.menu3}}</div>
            <div class="description">{{category.values.length}} datasets</div>
          </div>
        {{/link-to}}
      {{/each}}
    </div>  
  </div>
{{/liquid-bind}}

{{outlet}}
